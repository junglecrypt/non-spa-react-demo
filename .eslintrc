{
  "extends": "eslint:recommended",
  "rules": {
    "eol-last": 2,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "quotes": [2, "single"],
    "comma-dangle": 0,
    "no-console": 0,
    "no-trailing-spaces": [2, { "skipBlankLines": true }],
    "jsx-quotes": [2, "prefer-double"],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ]
}
